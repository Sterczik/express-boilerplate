{
  "name": "express-boilerplate",
  "version": "0.2.0",
  "description": "Simple Node, Express, MongoDB boilerplate",
  "keywords": [
    "node",
    "express",
    "mongo",
    "boilerplate"
  ],
  "main": "./src/app.js",
  "scripts": {
    "start": "node --exec babel-node ./src/app.js",
    "dev": "nodemon --exec babel-node ./src/app.js",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint ./src --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit"
  },
  "repository": "git@github.com:Sterczik/express-boilerplate.git",
  "author": "sterczik <sterczewskik@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "0.18.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "express-session": "1.15.6",
    "helmet": "3.13.0",
    "joi": "13.6.0",
    "method-override": "3.0.0",
    "moment": "^2.22.2",
    "mongoose": "5.2.13",
    "node-sass-middleware": "0.11.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.3"
  }
}
